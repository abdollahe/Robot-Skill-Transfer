</gazebo>

<gazebo reference="link6">
    <gravity>true</gravity>
    <self_collide>false</self_collide>
    <material value="Gazebo/Yellow"/>
    <!-- minDepth for magnetic gripper -->
    <minDepth>0.02</minDepth>
</gazebo>

<gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/schunk</robotNamespace>
  </plugin>
</gazebo>


<!--===================================================+==========Schunk Arm====================================================================================-->
<!--===================================================+========= Schunk arm connection to world ====================================================================================-->

<!-- Used for fixing robot to world -->
<link name="world"/>
<!--================================================================Schunk Arm Base===============================================================================-->

<link name="base_link">
    <visual>
        <origin xyz ="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="package://schunk_robot/meshes/robot_base_v3.STL"/>
        </geometry>
        <material name="red">
            <color rgba="1 0 0 1"/>
        </material>
        <!-- Gazebo visual plugin for changing the background of the scene -->
        <plugin name="visual_scene_edit" filename="libvisual_scene_edit.so"> 
        
        </plugin> 
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
            <mesh filename="package://schunk_robot/meshes/robot_base_v3.STL"/>
      </geometry>
    </collision>
    <inertial>
        <!--mass value="2"/-->
        <mass value="7"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.007988" ixy="0.000000" ixz="0.000000"
                 iyx="0.000000" iyy="0.004975" iyz="0.000000"
                 izx="0.000000" izy="0.000000" izz="0.008000"/>
    </inertial>
</link>

<joint name="arm_to_world_joint" type="fixed">
     <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0.0 0 0.0" rpy="0 0 0"/>
</joint>

<!--================================================================Link 1====================================================================================-->

