        <origin xyz="0 0 0.16" rpy="0 0 0"/>
        <inertia ixx="0.012578" ixy="0.000868" ixz="0.000291"
                 iyx="0.000868" iyy="0.005680" iyz="0.002500"
                 izx="0.000291" izy="0.002500" izz="0.011852"/>
