    <visual>
        <origin xyz="0 0 0.165" rpy="-1.57079 0 0"/>
        <geometry>
            <mesh filename="package://schunk_robot/meshes/joint1_v3.STL"/>
        </geometry>
        <material name="green">
            <color rgba="0 1 0 1"/>
        </material>
    </visual>
    <collision>
      <origin rpy="-1.57079 0 0" xyz="0 0 0.165"/>
      <geometry>
            <mesh filename="package://schunk_robot/meshes/joint1_v3.STL"/>
